<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yorijori.cook.classDAO.ClassCurriculumDAO">

	<insert id="insertCurriculum" parameterType="ClassCurriculumDTO">
		INSERT INTO CLASS_CURRICULUM
		(CLASS_ID, CUR_DAY, CUR_CONTENT)
		VALUES
		(#{CLASS_ID}, #{CUR_DAY}, #{CUR_CONTENT}) 
	</insert>

</mapper>