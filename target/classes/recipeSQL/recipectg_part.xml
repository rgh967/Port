<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="RecipeCtgParts">
	<select id="selectRecipeCtgId" parameterType ="int" resultType="int">
		select RECIPE_ID
		from RECIPE_CTG_PART
		where PART_ID = #{part}
	</select>
</mapper>